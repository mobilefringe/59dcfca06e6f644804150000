                <li><a href="#" id="mobile_leasing">LEASING</a></li>
                <li class="hidden_now mobile_menu_leasing"><a href="/leasing">Leasing</a></li>
                <li class="hidden_now mobile_menu_leasing"><a href="/photos">Photos</a></li>
                <li><a href="#" id="mobile_contact">CENTER INFORMATION</a></li>
                <li class="hidden_now mobile_menu_contact"><a href="/contact_us">Contact Us</a></li>
                <li class="hidden_now mobile_menu_contact"><a href="/location">Find Us</a></li>
                <li class="hidden_now mobile_menu_contact"><a href="/about_us">About Us</a></li>
                <li class="hidden_now mobile_menu_contact"><a href="/jobs">Jobs</a></li>
                <li class="hidden_now mobile_menu_contact"><a href="/hours">Shopping Hours</a></li>
                <li class="hidden_now mobile_menu_contact"><a href="/community">Community</a></li>
            </ul>
        </div>
        <div class="row">
            <div class="logo_container">
                <a href="/"><img src="//codecloud.cdn.speedyrails.net/sites/59dcfca06e6f644804150000/image/png/1509133590000/eglintonsquarelogo-01.png" class="main_logo" alt="Eglinton Square"/></a>
            </div>
            <div class="nav_container hidden_phone">
                <div class="social_links">
                    <a href="//www.facebook.com/eglintonsquareshoppingcentre" target="_blank">
                        <span id="social_facebook"></span>
                        <p class="display:none;">Facebook</p>
                    </a>
                     <a href="//www.instagram.com/eglintonsquare/" target="_blank">
                        <span id="social_insta"></span>
                        <p class="display:none;">Facebook</p>
                    </a>
                    <a href="//twitter.com/EglintonSquare" target="_blank">
                        <span id="social_twitter"></span>
                        <p class="display:none;">Facebook</p>
                    </a>
                    <a href="//www.pinterest.ca/eglintonsquare/" target="_blank">
