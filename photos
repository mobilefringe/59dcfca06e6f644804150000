<script>
    $(document).on('click', '[data-toggle="lightbox"]', function(event) {
        event.preventDefault();
        $(this).ekkoLightbox();
    });
    $(document).ready(function(e){
        init(e);
        loadMallDataCached(renderPage);
    });
    function renderPage(){
        var leasing_photos = getRepoDetailsByName("photos").images;
        renderGeneral("#portfolio_container", "#portfolio_template", leasing_photos);

        show_content();
    }
</script>